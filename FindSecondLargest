
import java.util.*;
import java.lang.*;
import java.io.*;

/* Name of the class has to be "Main" only if the class is public. */
class Codechef
{
	public static void main (String[] args) throws java.lang.Exception
	{
		Scanner sc= new Scanner(System.in);
		int arr[] =new int[3];
		for(int i=0; i<3; i++){
		int a= sc.nextInt();
        arr[i] = a;
		}
		Arrays.sort(arr);
		System.out.println(arr[1]);
	}
}

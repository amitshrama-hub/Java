import java.util.*;
public class Main
{
	public static void main(String[] args) {
	    Scanner sc = new Scanner(System.in);
	    int num = sc.nextInt();
	    int reversedValue = 0;
	    while(num != 0){
	        int reminder = num%10;
	        reversedValue = reversedValue*10 + reminder;
	        num /= 10;
	    }
	    System.out.println(reversedValue);
	}
}

import java.util.*;
public class Main
{
public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int bin = sc.nextInt();
    int dec = 0;
        int i = 0;
        while(bin != 0){
            int mod = bin%10;
            dec += mod*Math.pow(2,i);
            bin = bin/10;
            
            i++;
        }
        System.out.println(dec);
     }
  }

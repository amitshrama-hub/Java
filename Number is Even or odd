import java.util.Scanner;
public class Main
{
    public static void main(String args[])
    {
        int a;
        Scanner scc = new Scanner(System.in);
        System.out.println("Enter Number: ");
        a = scc.nextInt();
        if (a % 2 == 0)
        System.out.println("Even");
        else
        System.out.println("Odd");
    }
}

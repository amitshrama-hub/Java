import java.util.Scanner;
public class Main
{
public static void main(String args[])
{
    int num, a = 0, x = 0;
    Scanner sc = new Scanner(System.in);
    System.out.println("Enter the number: ");
    num = sc.nextInt();
    while (num > 0)
    {
        a = (a*10) + (num%10);
        num = num/10;
    }
    System.out.println(a);
}
}

import java.util.*;
public class Main
{
	public static void main(String[] args) {
		Scanner sc= new Scanner(System.in);
		System.out.print("Base : ");
		int num = sc.nextInt();
    System.out.print("Power : ");
    int pow = sc.nextInt();
    int ans = 1;
        for(int j = 0; j<pow; j++){
            ans *= num;
    }
    System.out.println("Answer : "+ans);
  }
}
